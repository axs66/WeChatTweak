<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
<dict>
    <key>PreferenceSpecifiers</key>
    <array>
        <!-- 设置分组标题 -->
        <dict>
            <key>Type</key>
            <string>PSGroupSpecifier</string>
            <key>Title</key>
            <string>WeChatTweak 设置</string>
        </dict>
        
        <!-- 多开功能启用/禁用 -->
        <dict>
            <key>Type</key>
            <string>PSSwitchSpecifier</string>
            <key>Title</key>
            <string>启用多开功能</string>
            <key>Key</key>
            <string>enableMultiAccount</string>
            <key>DefaultValue</key>
            <true/>
        </dict>
        
        <!-- 消息撤回拦截启用/禁用 -->
        <dict>
            <key>Type</key>
            <string>PSSwitchSpecifier</string>
            <key>Title</key>
            <string>启用消息撤回拦截</string>
            <key>Key</key>
            <string>enableMessageRevoke</string>
            <key>DefaultValue</key>
            <true/>
        </dict>

        <!-- 无后台消息显示详情 -->
        <dict>
            <key>Type</key>
            <string>PSSwitchSpecifier</string>
            <key>Title</key>
            <string>启用无后台消息显示详情</string>
            <key>Key</key>
            <string>enableNoBackgroundMessageDetails</string>
            <key>DefaultValue</key>
            <true/>
        </dict>
    </array>
</dict>
</plist>
